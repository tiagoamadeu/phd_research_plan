	number = {5},
	urldate = {2024-11-21},
	journal = {The Cryosphere},
	author = {Reinosch, Eike and Buckel, Johannes and Dong, Jie and Gerke, Markus and Baade, Jussi and Riedel, Björn},
	month = may,
	year = {2020},
	pages = {1633--1650},
	file = {Reinosch et al. - 2020 - InSAR time series analysis of seasonal surface dis.pdf:C\:\\Users\\tiago\\Zotero\\storage\\VQZRPLSQ\\Reinosch et al. - 2020 - InSAR time series analysis of seasonal surface dis.pdf:application/pdf},
}

