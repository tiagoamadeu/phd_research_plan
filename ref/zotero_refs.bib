@article{reinoschInSARTimeSeries2020,
  title = {{{InSAR}} Time Series Analysis of Seasonal Surface Displacement Dynamics on the {{Tibetan Plateau}}},
  author = {Reinosch, Eike and Buckel, Johannes and Dong, Jie and Gerke, Markus and Baade, Jussi and Riedel, Bj{\"o}rn},
