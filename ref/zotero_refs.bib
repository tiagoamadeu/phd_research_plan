@article{chaulagainComparativeAnalysisSlope2023,
  title = {A {{Comparative Analysis}} of {{Slope Failure Prediction Using}} a {{Statistical}} and {{Machine Learning Approach}} on {{Displacement Data}}: {{Introducing}} a {{Tailored Performance Metric}}},
  shorttitle = {A {{Comparative Analysis}} of {{Slope Failure Prediction Using}} a {{Statistical}} and {{Machine Learning Approach}} on {{Displacement Data}}},
