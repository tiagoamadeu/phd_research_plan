	journal = {Bulletin of Engineering Geology and the Environment},
	author = {Mirmazloumi, S. Mohammad and Wassie, Yismaw and Nava, Lorenzo and Cuevas-González, María and Crosetto, Michele and Monserrat, Oriol},
