	pages = {94},
	file = {Orellana et al. - 2023 - SAR Interferometry Data Exploitation for Infrastru.pdf:C\:\\Users\\tiago\\Zotero\\storage\\SUGE3PQT\\Orellana et al. - 2023 - SAR Interferometry Data Exploitation for Infrastru.pdf:application/pdf},
