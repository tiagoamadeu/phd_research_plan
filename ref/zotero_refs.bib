@article{xie2022,
  title = {A Multi-Platform, Open-Source, and Quantitative Remote Sensing Framework for Dam-Related Hazard Investigation: {{Insights}} into the 2020 {{Sardoba}} Dam Collapse},
  shorttitle = {A Multi-Platform, Open-Source, and Quantitative Remote Sensing Framework for Dam-Related Hazard Investigation},
