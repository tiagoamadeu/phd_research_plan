@article{dai2022,
  title = {Landslide {{Identification}} and {{Gradation Method Based}} on {{Statistical Analysis}} and {{Spatial Cluster Analysis}}},
  author = {Dai, Huayan and Zhang, Hong and Dai, Huayang and Wang, Chao and Tang, Wei and Zou, Lichuan and Tang, Yixian},
