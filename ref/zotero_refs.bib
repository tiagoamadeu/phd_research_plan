  shorttitle = {Cascading {{Landslide}}},
  author = {Zito, Claudia and Mangifesta, Massimo and Francioni, Mirko and Guerriero, Luigi and Di Martire, Diego and Calcaterra, Domenico and Sciarra, Nicola},
