	file = {Royán et al. - 2015 - Progressive failure leading to the 3 December 2013.pdf:C\:\\Users\\tiago\\Zotero\\storage\\55K9CRSQ\\Royán et al. - 2015 - Progressive failure leading to the 3 December 2013.pdf:application/pdf},
